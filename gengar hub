--LOADING HUB 
loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "GENGAR HUB" .. Fluent.Version,
    TabWidth = 160, Size = UDim2.fromOffset(580, 460), Theme = "Dark"
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}
--PAREGRAFOS
Tabs.Main:AddParagraph({ Title = "GENGAR COMUNITY", Content = "This is a paragraph.\nSecond line!" })

--BUTOES
Tabs.Main:AddButton({ Title = "LORD", Callback = function() 
loadstring(game:HttpGet('https://pastebin.com/raw/NqGgGjvD'))()
end })

Tabs.Main:AddButton({ Title = "ACHROMATIC", Callback = function()
loadstring(game:HttpGet("https://pastebin.com/raw/KqcTFiad"))()
end })
